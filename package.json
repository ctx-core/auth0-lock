{
	"name": "@ctx-core/auth0-lock",
	"version": "14.0.0",
	"description": "ctx-core auth0-lock",
	"keywords": [
		"ctx-core",
		"auth0",
		"auth0-lock"
	],
	"homepage": "https://github.com/ctx-core/auth0-lock#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-lock/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-lock.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^28.0.0",
		"@ctx-core/env": "workspace:^15.0.0",
		"@ctx-core/error": "workspace:^11.1.17",
		"@ctx-core/function": "workspace:^20.2.20",
		"@ctx-core/nanostores": "workspace:^0.1.0",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/package": "workspace:^9.1.20",
		"auth0-lock": "^11.31.1",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.121",
		"@types/auth0": "^2.34.8",
		"@types/auth0-js": "^9.14.5",
		"@types/auth0-lock": "^11.27.3",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
