{
	"name": "@ctx-core/auth0-lock",
	"version": "11.0.3",
	"description": "ctx-core auth0-lock",
	"keywords": [
		"ctx-core",
		"auth0",
		"auth0-lock"
	],
	"homepage": "https://github.com/ctx-core/auth0-lock#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-lock/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-lock.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^24.0.4",
		"@ctx-core/env": "workspace:^12.0.3",
		"@ctx-core/error": "workspace:^10.0.1",
		"@ctx-core/function": "workspace:^19.1.1",
		"@ctx-core/object": "workspace:^20.0.2",
		"@ctx-core/package": "workspace:^8.0.1",
		"@ctx-core/store": "workspace:^26.0.3",
		"auth0-lock": "^11.30.2",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^6.0.2",
		"@types/auth0": "^2.33.3",
		"@types/auth0-js": "^9.14.4",
		"@types/auth0-lock": "^11.27.1",
		"typescript": "^4.3.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}