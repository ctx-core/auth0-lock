{
	"name": "@ctx-core/auth0-lock",
	"version": "12.0.105",
	"description": "ctx-core auth0-lock",
	"keywords": [
		"ctx-core",
		"auth0",
		"auth0-lock"
	],
	"homepage": "https://github.com/ctx-core/auth0-lock#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-lock/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-lock.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run generate_ctx_I_file && npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"generate_ctx_I_file": "swc $(./node_modules/.bin/generate_ctx_I_file | tail -n1) --out-dir dist --source-maps --config-file .swcrc"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^25.0.103",
		"@ctx-core/env": "workspace:^13.0.45",
		"@ctx-core/error": "workspace:^11.0.18",
		"@ctx-core/function": "workspace:^20.2.3",
		"@ctx-core/object": "workspace:^21.0.22",
		"@ctx-core/package": "workspace:^9.1.0",
		"@ctx-core/store": "workspace:^27.0.36",
		"auth0-lock": "^11.31.1",
		"svelte": "3.44.1"
	},
	"devDependencies": {
		"@ctx-core/dev-tools": "workspace:^7.2.0",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.107",
		"@types/auth0": "^2.34.5",
		"@types/auth0-js": "^9.14.5",
		"@types/auth0-lock": "^11.27.3",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
