{
	"name": "@ctx-core/auth0-lock",
	"version": "13.0.0",
	"description": "ctx-core auth0-lock",
	"keywords": [
		"ctx-core",
		"auth0",
		"auth0-lock"
	],
	"homepage": "https://github.com/ctx-core/auth0-lock#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-lock/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-lock.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^27.0.0",
		"@ctx-core/env": "workspace:^14.0.0",
		"@ctx-core/error": "workspace:^11.1.9",
		"@ctx-core/function": "workspace:^20.2.12",
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/package": "workspace:^9.1.11",
		"@ctx-core/store": "workspace:^27.0.46",
		"auth0-lock": "^11.31.1",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"@types/auth0": "^2.34.7",
		"@types/auth0-js": "^9.14.5",
		"@types/auth0-lock": "^11.27.3",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
