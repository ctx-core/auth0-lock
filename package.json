{
	"name": "@ctx-core/auth0-lock",
	"version": "14.0.7",
	"description": "ctx-core auth0-lock",
	"keywords": [
		"ctx-core",
		"auth0",
		"auth0-lock"
	],
	"homepage": "https://github.com/ctx-core/auth0-lock#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-lock/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-lock.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^28.0.10",
		"@ctx-core/env": "workspace:^15.0.7",
		"@ctx-core/error": "workspace:^11.1.20",
		"@ctx-core/function": "workspace:^20.2.21",
		"@ctx-core/nanostores": "workspace:^0.3.1",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/package": "workspace:^9.1.21",
		"auth0-lock": "^11.31.1",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.125",
		"@types/auth0": "^2.34.9",
		"@types/auth0-js": "^9.14.5",
		"@types/auth0-lock": "^11.27.3",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js"
}
